import java.util.Scanner;

class GenIdxArray {

	int[] idxArray(int n){
		int[] a = new int[n];
		for ( int i = 0 ; i < n ; i++)
		a[i] = i;
		return a;
	}

	public static void main(String[] args) {
		Scanner stdln = new Scanner(System.in);
		GenIdxArray ob = new GenIdxArray();

		System.out.print("요소 수는 : ");
		int n = stdln.nextInt();
		int[] x = ob.idxArray(n);

		for ( int i = 0 ; i < n ; i++ ){
			System.out.println("x[" + i + "] = " + x[i]);
		}
	}
}
